{
  "manifest_version": 2,
  "name": "UJust Extension",
  "short_name": "Unjust",
  "description": "Discourages the use of certain words that undermine women",
  "author": "",
  "version": "1.0",
},
"content_scripts": [
  {
    "matches": [
      "*://*/*"
    ],
    "all_frames": true,
    "js": [
      "content_script.js"
    ],
    "run_at": "document_end"
  }
],
"browser_action": {
  "default_title": "UnJust",
  "default_popup": "popup.html"
}
}